<h1>マイチームページ</h1>
<div class="row">
  <div class="col-10">


  <table class="table table-striped table-bordered bg-light">
      <tbody>
          <tr>
              <th width="20%">チーム名</th>
              <td><%= @admin.team_name %></td>
          </tr>
          <tr>
              <th>活動場所</th>
              <td><%= @admin.address %></td>
          </tr>
          <tr>
              <th>ジャンル</th>
              <td><%= @admin.genre %></td>
          </tr>
          <tr>
              <th>メールアドレス</th>
              <td><%= @admin.email %></td>
          </tr>
      </tbody>
  </table>

  <%= link_to "マイチームの内容を変更する", edit_admins_admin_path(@admin), class: "btn btn-primary mb-4" %><br>
</div>